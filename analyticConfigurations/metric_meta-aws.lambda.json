{
  "analyticConfiguration": {
    "metrics": [
      {
        "match": "aws.lambda.duration",
        "properties": {
          "SparseDataStrategy": "ReplaceWithZero",
          "tags": {
            "unit": "ms"
          }
        }
      },
      {
        "match": "aws.lambda.(errors|invocations|throttles)",
        "properties": {
          "SparseDataStrategy": "ReplaceWithZero",
          "statistic": "sum",
          "tags": {
            "unit": "count"
          }
        }
      },
      {
        "match": "netuitive.*",
        "properties": {
          "tags": {
            "unit": "percent"
          },
          "validMax": 100
        }
      },
      {
        "match": "aws.lambda.memory.utilization",
        "properties": {
          "tags": {
            "unit": "percent"
          },
          "validMax": 100
        }
      },
      {
        "match": "aws.lambda.billed",
        "properties": {
          "tags": {
            "unit": "GB-S"
          }
        }
      }
    ],
    "name": "AWS Lambda",
    "scope": {
      "elementType": "LAMBDA"
    },
    "type": "METRIC_META"
  }
}